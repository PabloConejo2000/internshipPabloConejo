{
  "name": "ember-data",
  "version": "5.3.3",
  "description": "The lightweight reactive data library for JavaScript applications",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:emberjs/data.git",
    "directory": "packages/-ember-data"
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "files": [
    "dist/docs",
    "ember-data-logo-dark.svg",
    "ember-data-logo-light.svg",
    "LICENSE.md",
    "README.md",
    "index.js",
    "addon",
    "addon-test-support",
    "app",
    "blueprints"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@ember/string": "^3.1.1"
  },
  "dependencies": {
    "@ember-data/adapter": "5.3.3",
    "@ember-data/debug": "5.3.3",
    "@ember-data/graph": "5.3.3",
    "@ember-data/json-api": "5.3.3",
    "@ember-data/legacy-compat": "5.3.3",
    "@ember-data/model": "5.3.3",
    "@ember-data/private-build-infra": "5.3.3",
    "@ember-data/request": "5.3.3",
    "@ember-data/request-utils": "5.3.3",
    "@ember-data/serializer": "5.3.3",
    "@ember-data/store": "5.3.3",
    "@ember-data/tracking": "5.3.3",
    "@ember/edition-utils": "^1.2.0",
    "@embroider/macros": "^1.13.5",
    "@warp-drive/core-types": "0.0.0-beta.4",
    "broccoli-merge-trees": "^4.2.0",
    "ember-auto-import": "^2.7.0",
    "ember-cli-babel": "^8.2.0",
    "ember-inflector": "^4.0.2",
    "typescript": "^5.3.3",
    "pnpm-sync-dependencies-meta-injected": "0.0.10",
    "webpack": "^5.89.0"
  },
  "dependenciesMeta": {
    "@ember-data/adapter": {
      "injected": true
    },
    "@ember-data/graph": {
      "injected": true
    },
    "@ember-data/debug": {
      "injected": true
    },
    "@ember-data/model": {
      "injected": true
    },
    "@ember-data/json-api": {
      "injected": true
    },
    "@ember-data/request": {
      "injected": true
    },
    "@ember-data/request-utils": {
      "injected": true
    },
    "@ember-data/legacy-compat": {
      "injected": true
    },
    "@ember-data/serializer": {
      "injected": true
    },
    "@ember-data/store": {
      "injected": true
    },
    "@ember-data/tracking": {
      "injected": true
    },
    "@ember-data/private-build-infra": {
      "injected": true
    },
    "@ember/string": {
      "injected": true
    },
    "@warp-drive/core-types": {
      "injected": true
    },
    "ember-inflector": {
      "injected": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@ember/string": "3.1.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@warp-drive/internal-config": "5.3.3",
    "ember-source": "~5.6.0",
    "eslint": "^8.56.0"
  },
  "engines": {
    "node": ">= 18.19.1"
  },
  "keywords": [
    "ember-addon"
  ],
  "ember-addon": {
    "version": 2,
    "auto-upgraded": true,
    "type": "addon",
    "order-index": 105,
    "renamed-modules": {},
    "implicit-modules": [
      "./-private/core",
      "./-private/index",
      "./adapter",
      "./adapters/errors",
      "./adapters/json-api",
      "./adapters/rest",
      "./attr",
      "./index",
      "./model",
      "./relationships",
      "./serializer",
      "./serializers/embedded-records-mixin",
      "./serializers/json-api",
      "./serializers/json",
      "./serializers/rest",
      "./setup-container",
      "./store",
      "./transform",
      "./version"
    ],
    "implicit-test-modules": [
      "./test-support/index"
    ],
    "app-js": {
      "./initializers/ember-data.js": "./_app_/initializers/ember-data.js",
      "./services/store.js": "./_app_/services/store.js",
      "./transforms/boolean.js": "./_app_/transforms/boolean.js",
      "./transforms/date.js": "./_app_/transforms/date.js",
      "./transforms/number.js": "./_app_/transforms/number.js",
      "./transforms/string.js": "./_app_/transforms/string.js"
    }
  },
  "ember": {
    "edition": "octane"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "packageManager": "pnpm@8.6.0",
  "scripts": {
    "lint": "eslint . --quiet --cache --cache-strategy=content --ext .js,.ts,.mjs,.cjs --report-unused-disable-directives",
    "move-dts": "bun ../../scripts/copy-declarations.mjs addon",
    "build:types": "echo \"Types are private\" && exit 0",
    "_build": "bun run build:types && bun run move-dts",
    "_syncPnpm": "bun run sync-dependencies-meta-injected"
  }
}