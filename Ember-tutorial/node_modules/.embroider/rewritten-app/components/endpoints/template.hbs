<div>
    <div class="mt-3 flex items-center justify-center">
        <h1 class="italic text-2xl">Testing information from mock</h1>

    </div>
    <div class="mt-3 flex items-center justify-center">
        <h1>The following information is the default info, push the button to update it from my personal endpoint</h1>
    </div>

    <div class="mt-3 flex items-center justify-center">
        <h2 {{did-insert this.writeData}} {{did-update this.writeData this.buttonState }}>
            Name: {{this.dogName}} ---- Breed: {{this.dogBreed}}</h2>

    </div>

    <div class="mt-3 flex items-center justify-center">
        <button
            class="bg-cyan-500 shadow-lg shadow-cyan-500/50 hover:bg-sky-700  text-black font-bold py-2 px-4 border border-black rounded ml-2"
            {{on "click" (fn this.changeButtonState true )}}>Upater</button>

    </div>

</div>